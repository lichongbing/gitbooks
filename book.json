{
    "title": "Java笔记Blog",
    "author": "snowdreams1006",
    "description": "gitbook-plugin-simple-mind-map examples",
    "gitbook": "3.2.3",
    "links": {
        "sidebar": {
            "blog": "https://blog.lichongbing.com/"
        }
    },
    "plugins": [
        "simple-mind-map",
        "new-flowchart",
        "mermaid-gb3@2.1.0",
        "hide-element",
        "-lunr",
        "-search",
        "search-plus@^0.0.11",
        "back-to-top-button",
        "openwrite",
        "code",
        "prism",
        "-highlight",
        "-livereload",
        "expandable-chapters",
        "timeline",
        "todo",
        "html5-video"
    ],
    "pluginsConfig": {
        "simple-mind-map": {
            "type": "markdown",
            "preset": "colorful",
            "linkShape": "diagonal",
            "autoFit": true,
            "style": {
                "height": "750px",
                "wight": "1000px"
            }
        },
        "hide-element": {
            "elements": [
                ".gitbook-link"
            ]
        },
        "openwrite": {
            "blogId": "18972-1576749538667-677",
            "name": "谦谦君子",
            "qrcode": "https://image.lichongbing.com/qrcode_for_gh_29bad7d68f3a_258.jpg",
            "keyword": "更多"
        },
        "prism": {
            "css": [
                "prismjs/themes/prism-tomorrow.css"
            ]
        }
    }
}