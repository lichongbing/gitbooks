{
    "title": "simple-mind-map examples",
    "author": "snowdreams1006",
    "description": "gitbook-plugin-simple-mind-map examples",
    "gitbook": "3.2.3",
    "links": {
        "sidebar": {
            "blog": "https://blog.lichongbing.com/"
        }
    },
    "plugins": [
        "simple-mind-map",    
        "new-flowchart",
        "mermaid-gb3@2.1.0",
        "hide-element"
    ],
    "pluginsConfig": {
        "simple-mind-map": {
            "type": "markdown",
            "preset": "colorful",
            "linkShape": "diagonal",
            "autoFit": true,
            "style": {
                "height":  "750px",
                "wight":  "1000px"
              } 
        },
        "hide-element": {
            "elements": [".gitbook-link"]
        }

       
        
    }
}